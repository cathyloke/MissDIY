{
    "private": true,
    "scripts": {
        "dev": "npm run development",
        "development": "mix",
        "watch": "mix watch",
        "watch-poll": "mix watch -- --watch-options-poll=1000",
        "hot": "mix watch --hot",
        "prod": "npm run production",
        "production": "mix --production",
        "setup": "bash ./setup.sh",
        "dropdb": "php artisan db:wipe",
        "migrate": "bash ./dbMigration.sh",
        "seed": "php artisan db:seed",
        "serve": "php artisan serve",
        "serveb": "start php artisan serve  && start http://127.0.0.1:8000"
    },
    "devDependencies": {
        "axios": "^0.21",
        "laravel-mix": "^6.0.6",
        "lodash": "^4.17.19",
        "postcss": "^8.1.14"
    }
}
